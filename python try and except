def main():
    print("Starting The Code Challenge")

    print("Starting <Specific Part> Of Code Challenge")

    # Example 1: Try / Except
    try:
        print(x)
    except:
        print("An error occurred")

    # Example 2: Try / Except with specific error
    try:
        x = 42 / 0
        print(x)
    except ZeroDivisionError:
        print("Cannot divide by zero")

    # Example 3: Try / Except / Else
    try:
        x = 10
        print(x)
    except:
        print("Something went wrong")
    else:
        print("Nothing went wrong")

    # Example 4: Try / Except / Finally
    try:
        x = 5
        print(x)
    except:
        print("Something went wrong")
    finally:
        print("The try except is finished")

    print("Ending <Specific Part> Of Code Challenge")
    print("Ending The Code Challenge")

main()
